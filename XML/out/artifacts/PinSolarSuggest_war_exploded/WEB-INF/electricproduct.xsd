<?xml version="1.0"?>
<xsd:schema version="1.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            elementFormDefault="qualified">
    <xsd:element name="electricproductentity">
        <xsd:complexType>
            <xsd:all>
                <xsd:element type="xsd:int" name="idProduct"/>
                <xsd:element type="xsd:string" name="productName"/>
                <xsd:element type="xsd:double" name="productCapacity"/>
                <xsd:element type="xsd:int" name="idCategory"/>
                <xsd:element type="xsd:int" name="idBrand"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>